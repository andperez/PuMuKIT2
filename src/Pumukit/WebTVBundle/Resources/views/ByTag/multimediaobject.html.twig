{% if object.getRecordDate()|date("d/m/Y") != lastDate %}
  <div style="clear:both;"></div>
  <br />
  <div class="serial_date">
    {{ object.getRecordDate()|date("d/m/Y") }}
  </div>
{% endif %}

<!--Objeto multimedia-->
<div>
  <div class="pumukit_mmobjs" style="width:50%; float:left;">
    <div class="pumukit_mmobj" style="float:left;">
      <div class="picture" style="position: relative;
        width: 130px;
        height: 110px;
        float: left;
        margin: 10px;
        margin-right: 20px;">
        <a href="{{ path('pumukit_webtv_multimediaobject_index', {'id': object.getId()}) }}">
          <img src="{{ object|first_url_pic(true) }}"></img>
        </a>
      </div>
      <div class="info">
        <div class="title">
          <a href="{{ path('pumukit_webtv_multimediaobject_index', {'id': object.getId()}) }}">{{object.getTitle()}}</a>
        </div>

        <div class="subtitle">
          <abbr title="{{object.getSubtitle()}}">{{object.getSubtitle()}}</abbr>
        </div>

        <div class="mmobj_bottom">{{object.getRecordDate()|date("d/m/Y")}}</div>
      </div>
    </div>
  </div>
</div>
